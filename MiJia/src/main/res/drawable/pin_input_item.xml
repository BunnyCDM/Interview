<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_selected="false">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="1.0px" android:color="#ffb8b8b8" />
                    <solid android:color="@color/white" />
                    <size android:height="47.0dip" android:width="47.0dip" />
                </shape>
            </item>
            <item android:gravity="center" android:left="16.5dip" android:top="16.5dip" android:right="16.5dip" android:bottom="16.5dip">
                <shape android:shape="oval">
                    <solid android:color="#ff191919" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_enabled="false" android:state_selected="false">
