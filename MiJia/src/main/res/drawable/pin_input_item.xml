                    <size android:height="36.0dip" android:width="2.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_enabled="false" android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0px" android:color="#ff00bc9c" />
            <solid android:color="@color/white" />
            <size android:height="47.0dip" android:width="47.0dip" />
        </shape>
    </item>
</selector>