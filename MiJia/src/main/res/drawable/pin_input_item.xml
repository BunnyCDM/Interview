            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="1.0px" android:color="#ff00bc9c" />
                    <solid android:color="@color/white" />
                    <size android:height="47.0dip" android:width="47.0dip" />
                </shape>
            </item>
            <item android:gravity="center" android:left="22.5dip" android:top="5.5dip" android:right="22.5dip" android:bottom="5.5dip">
                <shape android:shape="rectangle">
                    <solid android:color="#ff00bc9c" />
