<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <solid android:color="#ffe54200" />
            <stroke android:width="1.0dip" android:color="#ffe54200" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <solid android:color="#ffe0e0e0" />
            <stroke android:width="1.0dip" android:color="#ffe0e0e0" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <solid android:color="@color/mishopsdk_orange" />
            <stroke android:width="1.0dip" android:color="@color/mishopsdk_orange" />
        </shape>
    </item>
</selector>